pipeline {
    agent any

    stages {
        stage('Build') {
            steps {
                sh "npm install"
                sh 'export JENKINS_NODE_COOKIE=dontKillMe'
                sh 'npm run start:dev &'
            }
        }
          stage('Test') {
            steps {
                sh "curl https://localhost:3000"
            }
        }
    }
}
