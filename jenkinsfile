pipeline {
    agent any

    stages {
        stage('Build') {
                 withEnv ( ['JENKINS_NODE_COOKIE=do_not_kill'] ) {
                 
                   bat " (npm install) && (npm run start:dev) &"
                }
        }
          stage('Test') {
            steps {
                sh "curl https://localhost:3000"
            }
        }
    }
}
